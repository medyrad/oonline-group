{
  "name": "online-group",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "bun --bun next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "biome lint --no-errors-on-unmatched .",
    "lint:fix": "biome lint --write --no-errors-on-unmatched .",
    "typecheck": "tsc --noEmit",
    "format": "biome format --write .",
    "mock": "json-server --watch src/mocks/db.json --port 4000 --delay 400 --id id",
    "socket": "bun src/mocks/socket-server.ts",
    "dev:full": "concurrently \"pnpm mock\" \"pnpm socket\" \"bun --bun next dev --turbopack\"",
    "prepare": "husky",
    "generate": "bun src/mocks/generate-orders.js",
    "test": "pnpm test:unit",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "e2e": "cypress open",
    "e2e:headless": "cypress run",
    "perf": "next build && lhci autorun",
    "check:ci": "biome ci --no-errors-on-unmatched .",
    "release": "semantic-release"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-slot": "^1.2.4",
    "@tanstack/react-query": "^5.90.11",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.19",
    "i18next": "^25.6.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "^0.555.0",
    "next": "16.0.4",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.67.0",
    "react-i18next": "^16.3.5",
    "recharts": "^3.5.1",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "valtio": "^2.2.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@faker-js/faker": "^10.1.0",
    "@next/bundle-analyzer": "^16.0.5",
    "@playwright/test": "^1.57.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.1.0",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/cypress": "^10.1.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "baseline-browser-mapping": "^2.8.32",
    "concurrently": "^9.2.1",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "cypress": "^15.7.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "json-server": "0.17.4",
    "lighthouse-ci": "^1.13.1",
    "lint-staged": "^16.2.7",
    "semantic-release": "^24.1.2",
    "socket.io": "^4.8.1",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "web-vitals": "^5.1.0"
  }
}
